{
  "manifest_version": 3,
  "name": "Crontabs",
  "description": "Easy Tab Scheduling for Chrome",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
